<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <club.bweakfast.foodora.custom.TwoLineText
        android:id="@+id/nutritionTxt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        tools:text1="234"
        tools:text2="CALORIES" />

    <club.bweakfast.foodora.custom.TwoLineText
        android:id="@+id/nutritionTxt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        tools:text1="40g"
        tools:text2="PROTEIN" />

    <club.bweakfast.foodora.custom.TwoLineText
        android:id="@+id/nutritionTxt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        tools:text1="25g"
        tools:text2="TOTAL FAT" />

    <club.bweakfast.foodora.custom.TwoLineText
        android:id="@+id/nutritionTxt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        tools:text1="12g"
        tools:text2="TOTAL CARBS" />

</merge>